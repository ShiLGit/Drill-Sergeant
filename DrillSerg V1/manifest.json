{
  "manifest_version": 2,
  "name": "Drill Sergeant",
  "icons": {"128": "images/icon128.png"},
  "description": "Regulates time spent on unproductive sites",
  "version": "1.2.2",
  "browser_action": {"default_popup": "popup.html", "default_icon": "images/icon128.png" },
  "background": {"scripts": ["background.js"]},
  "permissions": ["activeTab", "webNavigation", "notifications", "tabs", "background"],
  "web_accessible_resources": ["censor.html"]
}
